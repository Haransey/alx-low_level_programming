#include <stdio.h>

/**
 * main - main block
 * Description: Print the two digit numbers with comma and space
 * Return: 0
 */

int main(void)
{
	int a;
	int b;
	int c;
	int d;
	int g;
	int e;

	a = 0;
	g = 1;
	while (a < 100)
	{
		b = a / 10;
		c = a  % 10;
		if (a < 99)
		{
			putchar(b + '0');
			putchar(c + '0');
			putchar (32);
		}
		while (g < 100)
		{
			d = g / 10;
			e = g % 10;
			if (g < 99)
			{
				putchar(d + '0');
				putchar(e + '0');
				putchar(44);
				putchar(32);
			}
			g++;
		}
		a++;
	}
	putchar('\n');
	return (0);
}
